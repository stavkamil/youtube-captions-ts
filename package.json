{
  "name": "youtube-captions-ts",
  "version": "0.1.0",
  "description": "A TypeScript library for fetching YouTube captions",
  "author": "Stav Kamil",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/stavkamil/youtube-captions-ts/issues"
  },
  "homepage": "https://github.com/stavkamil/youtube-captions-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stavkamil/youtube-captions-ts.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "youtube",
    "captions",
    "transcript",
    "subtitles",
    "typescript"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "npx ts-node src/index.ts",
    "start": "npx ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "fast-xml-parser": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.3.5",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
